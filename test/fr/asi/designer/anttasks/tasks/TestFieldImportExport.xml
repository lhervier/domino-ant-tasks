<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="test-fieldExport" basedir="." default="test">
	<taskdef resource="userlessbuild.antlib.xml"/>
	<target name="test">
		<fieldExport 
				password="${password}"
				server="${server}" 
				database="${db}" 
				formula="Form = 'Export'"
				fields="IntField,DateField,Multi,RtItem"
				toFile="${file}"/>
		<fieldImport
				password="${password}"
				server="${server}" 
				database="${db}"
				formula="Form = 'Import'"
				fromFile="${file}"/>
	</target>
</project>